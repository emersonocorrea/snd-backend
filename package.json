{
  "name": "snd-hospitalar-backend",
  "version": "1.0.0",
  "description": "Backend for Sistema de Nutrição e Dietética Hospitalar",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "migrate": "npx knex migrate:latest --knexfile src/knexfile.js --env development",
    "migrate:prod": "npx knex migrate:latest --knexfile src/knexfile.js --env production",
    "seed": "npx knex seed:run --knexfile src/knexfile.js --env development",
    "seed:prod": "npx knex seed:run --knexfile src/knexfile.js --env production"
},
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}